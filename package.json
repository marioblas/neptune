{
  "name": "neptune",
  "version": "0.1.0",
  "description": "Neptune",
  "scripts": {
    "lint": "eslint ./client ./lib ./server --ext .js --ext .jsx",
    "lint:fix": "npm run lint -- --fix",
    "start": "meteor run --settings settings-development.json"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "bcrypt": "^1.0.1",
    "chalk": "^1.1.3",
    "mantra-core": "^1.7.0",
    "meteor-node-stubs": "^0.2.4",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-helmet": "^3.2.3",
    "react-mounter": "^1.2.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-core": "^6.20.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-2": "^6.18.0",
    "eslint": "^3.12.0",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-react": "^6.8.0",
    "node-sass": "^3.13.0",
    "pre-commit": "^1.2.0"
  },
  "cssModules": {
    "extensions": [
      "scss"
    ],
    "ignorePaths": [
      "node_modules"
    ],
    "postcssPlugins": {
      "postcss-modules-values": {},
      "postcss-modules-local-by-default": {},
      "postcss-modules-extract-imports": {},
      "postcss-modules-scope": {},
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      }
    }
  },
  "license": "MIT",
  "private": true
}
